{
  "name": "vector-graphics-app",
  "version": "0.1.0",
  "type": "module",
  "homepage": "https://monkeyapproved.github.io/VectorGraphics",
  "private": true,
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
